{{ 'gen-2-home-img-counter.css' | asset_url | stylesheet_tag }}

<div class="independently-tested gen-counter" id="independently-tested">
  <div class="container">
    <div class="independently-tested__wrap">
      <div class="independently-tested__image">
        <img
          class="desktop-image"
          src="{{ section.settings.desktop_image | img_url : 'master' }}"
          alt="Independently Tested Image"
        >
        <img
          class="mobile-image"
          src="{{ section.settings.mobile_image | img_url : 'master' }}"
          alt="Independently Tested Image"
        >
      </div>
      <div class="independently-tested__content">
        <!-- Stopsnoring Score Section -->
        <div class="stopsnoring__score">
          <div class="stopsnoring__score-progress">
            <div class="circle">
              <svg width="70" height="70" viewBox="0 0 200 200">
                <!-- Background 3/4 arc -->
                <path
                  d="M55,155 A75,75 0 1,1 145,155"
                  stroke="#eee"
                  stroke-width="36"
                  fill="none"
                  id="arc-bg"
                  stroke-linecap="round"
                />
                <!-- Progress arc -->
                <path
                  id="progress"
                  d="M55,155 A75,75 0 1,1 145,155"
                  stroke="#51DF83"
                  stroke-width="36"
                  fill="none"
                  stroke-linecap="round"
                />
              </svg>

              <div class="score" id="score">{{ section.settings.snore_score }}</div>
            </div>
          </div>
          <div class="stopsnoring__score-content">
            <p class="stopsnoring__score-title">Snore Score</p>
            <div class="stopsnoring__score-average">
              <span>
                <img
                  src="https://cdn.shopify.com/s/files/1/1053/4408/files/down-arrow.svg?v=1742799102"
                  alt="Down Arrow"
                >
                {{ section.settings.weekly_avg_percentage }}%
              </span>
              <span class="stopsnoring__score-weekly">Weekly Avg</span>
            </div>
          </div>
        </div>
        <!-- Last Night's Sleep Data -->
        <div class="stopsnoring__last-night">
          <div class="stopsnoring__last-night-box">
            <p class="stopsnoring__last-night-title white-text">{{ section.settings.night_data_title }}</p>
            <div class="stopsnoring__last-night-with">
              <img
                src="https://cdn.shopify.com/s/files/1/1053/4408/files/light-blue-pabble.svg?v=1742799102"
                alt="Light Blue Pebble"
              >
              With Nora
            </div>
          </div>
          <div class="sleep">
            <div class="sleep__entry">
              <div class="sleep__icon">
                <img src="https://cdn.shopify.com/s/files/1/1053/4408/files/moon.svg?v=1742799102" alt="Moon Icon">
              </div>
              <div class="sleep__data">
                <div
                  class="sleep-counter__text"
                >
                  <p>You Slept</p>
                  <h3 class="sleep-time">
                    <span class="hours">{{ section.settings.slept_hours }}</span><span class="h-sign">h</span>
                    <span class="minutes">{{ section.settings.slept_minutes }}</span><span class="m-sign">m</span>
                  </h3>
                </div>
              </div>
            </div>
            <div class="sleep__entry">
              <div class="sleep__icon">
                <img
                  src="https://cdn.shopify.com/s/files/1/1053/4408/files/light-green-pabble.svg?v=1742799102"
                  alt="Green Pebble"
                >
              </div>
              <div class="sleep__data">
                <div class="sleep-counter__text">
                  <p>Nora helped you</p>
                  <h3 class="sleep-time">
                    <span class="hours">{{ section.settings.nora_helped_times }} </span><span>times</span>
                  </h3>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Sleep Continuity Section -->
    <div class="sleep-continuity">
      <div class="sleep-continuity__header">
        <div class="sleep-continuity__title white-text">
          <span class="sleep-continuity__icon">
            <img src="https://cdn.shopify.com/s/files/1/1053/4408/files/white-icon.svg?v=1742799102" alt="White Icon">
          </span>
          {{ section.settings.pre_heading }}
        </div>
        <h2 class="sleep-continuity__subtitle white-text">{{ section.settings.heading }}</h2>
        <p class="sleep-continuity__description white-text">
          {{ section.settings.content }}
        </p>
      </div>
    </div>

    <!-- Counter Section -->
    <div class="sleep-continuity-counter">
      <div class="sleep-continuity-counter__box">
        <div class="sleep-continuity-counter__text">
          <svg version="1.2" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 37" width="32" height="37">
            <title>90%</title>
            <defs>
            	<image  width="136" height="39" id="img1" href="data:image/png;base64,"/>
            </defs>
            <style>
            </style>
            <use id="Background" href="#img1" x="0" y="-1"/>
          </svg>
          <span> {{ section.settings.loud_snoring_count }}%</span>
        </div>
        <div class="sleep-continuity-counter__content">
          <p>{{ section.settings.loud_snoring_title }}</p>
        </div>
      </div>
      <div class="sleep-continuity-counter__box">
        <div class="sleep-continuity-counter__text">
          <svg version="1.2" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 37" width="32" height="37">
            <title>90%</title>
            <defs>
            	<image  width="136" height="39" id="img1" href="data:image/png;base64,"/>
            </defs>
            <style>
            </style>
            <use id="Background" href="#img1" x="0" y="-1"/>
          </svg>
          <span> {{ section.settings.less_sleep_count }}X</span>
        </div>
        <div class="sleep-continuity-counter__content">
          <p>{{ section.settings.less_sleep_title }}</p>
        </div>
      </div>
      <div class="sleep-continuity-counter__box">
        <div class="counter__text--increase sleep-continuity-counter__text">
          <svg version="1.2" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 33 37" width="33" height="37">
            <title>90% (1)</title>
            <defs>
            	<image  width="135" height="39" id="img2" href="data:image/png;base64,"/>
            </defs>
            <style>
            </style>
            <use id="Background" href="#img2" x="0" y="-1"/>
          </svg>
          <span>{{- section.settings.more_partners_count }}%</span>
        </div>
        <div class="sleep-continuity-counter__content">
          <p>{{ section.settings.more_partners_title }}</p>
        </div>
      </div>
    </div>
    <!-- Button -->
    <div class="btn btn_green_outline">
      <a href="{{ section.settings.button_link }}" target="_blank">
        <span class="btn__icon">
          <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path fill-rule="evenodd" clip-rule="evenodd" d="M17.911 13.5673C17.7103 12.9418 17.4354 12.2836 17.0889 11.6075C16.3688 12.6155 15.5247 13.6153 14.5673 14.5726C13.6133 15.5266 12.6171 16.3683 11.6127 17.0867C12.288 17.4327 12.9456 17.7073 13.5705 17.9078C15.796 18.622 17.1128 18.2775 17.6967 17.6935C18.2806 17.1096 18.6252 15.7928 17.911 13.5673ZM13.3741 13.3794C14.4757 12.2778 15.4037 11.1292 16.147 9.99759C15.4042 8.8675 14.4774 7.72065 13.3773 6.62061C12.2781 5.52142 11.1322 4.59511 10.003 3.85259C8.87113 4.59599 7.72239 5.52407 6.62061 6.62585C5.52227 7.72419 4.59654 8.86919 3.85432 9.99759C4.59708 11.1275 5.52387 12.2742 6.62378 13.3741C7.72453 14.4749 8.87215 15.4023 10.0029 16.1453C11.1312 15.4032 12.276 14.4776 13.3741 13.3794ZM18.8899 18.8867C20.6608 17.1159 20.2371 13.6209 18.1309 9.99727C20.2344 6.37556 20.6567 2.88325 18.8867 1.11325C17.1169 -0.656623 13.6249 -0.234443 10.0035 1.8687C6.37881 -0.238986 2.88252 -0.663341 1.11117 1.10801C-0.659678 2.87886 -0.236053 6.37369 1.87011 9.99727C-0.238797 13.6228 -0.663734 17.1202 1.10801 18.892C2.87987 20.6639 6.37767 20.2387 10.0035 18.1295C13.626 20.2342 17.1195 20.6571 18.8899 18.8867ZM13.5673 2.09216C12.9435 2.29234 12.2873 2.56626 11.6132 2.91139C12.6185 3.63023 13.6156 4.47254 14.5705 5.4274C15.5262 6.38308 16.3691 7.38107 17.0884 8.38729C17.4336 7.71304 17.7076 7.05662 17.9078 6.43271C18.622 4.20721 18.2775 2.89039 17.6935 2.30646C17.1096 1.72252 15.7928 1.37795 13.5673 2.09216ZM6.43063 2.08692C7.05703 2.28794 7.7162 2.56333 8.39328 2.91053C7.38503 3.6307 6.38498 4.47508 5.4274 5.43265C4.47311 6.38694 3.63125 7.38342 2.9127 8.38816C2.56597 7.71176 2.29092 7.05326 2.09009 6.42746C1.37588 4.20197 1.72045 2.88514 2.30438 2.30121C2.88831 1.71728 4.20513 1.37271 6.43063 2.08692ZM2.08692 13.5725C2.28826 12.9451 2.5642 12.2849 2.91217 11.6067C3.63153 12.6132 4.47465 13.6114 5.43057 14.5673C6.38731 15.5241 7.38645 16.3678 8.3938 17.0876C7.71542 17.4357 7.055 17.7117 6.42746 17.9131C4.20197 18.6273 2.8